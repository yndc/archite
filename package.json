{
  "name": "transql",
  "description": "CLI and Node.js Library for data modeling around SQL database schemas",
  "version": "0.0.1",
  "author": "Jonathan Steven <yondercode@gmail.com>",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "dedent-js": "^1.0.1",
    "js-yaml": "^3.13.1",
    "knex": "^0.19.3",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "pluralize": "^8.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/jest": "^24.0.18",
    "@types/node": "^10",
    "globby": "^10",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "ts-node": "^8",
    "tspath": "^1.3.7",
    "typescript": "^3.3"
  },
  "license": "GPL-3.0-or-later",
  "main": "dist/libs/index.js",
  "prettier": {
    "semi": false
  },
  "files": [
    "/dist"
  ],
  "bin": {
    "transql": "./dist/run"
  },
  "oclif": {
    "commands": "./dist/cli/commands",
    "bin": "oclif-test",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/yndc/transql.git",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && tspath -f",
    "test": "jest -i"
  }
}
